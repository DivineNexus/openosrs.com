mat-toolbar.header(color='primary')
  mat-toolbar-row
    a.title(routerLink='/home')
      img.logo(src='data:image/png;base64,', alt='RunelitePlus')
      span RuneLitePlus

    a(mat-button='' routerLink='/updates', fxHide.xs="true") Updates

    a(mat-button='' routerLink='/features', fxHide.xs="true") Features

    span.spacer

    a(mat-icon-button, target='_blank', rel='noopener noreferrer', href='https://discord.gg/Q7wFtCe')
      mat-icon.icon(aria-hidden='false', aria-label='Discord', svgIcon='discord')
    a(mat-icon-button, target='_blank', rel='noopener noreferrer', href='https://twitter.com/runeliteplus')
      mat-icon.icon(aria-hidden='false', aria-label='Twitter', svgIcon='twitter')
    a(mat-icon-button, target='_blank' rel='noopener noreferrer', href='https://github.com/runelite-extended')
      mat-icon.icon(aria-hidden='false', aria-label='Github', svgIcon='github')
    a(mat-icon-button, rel='noopener noreferrer', href='https://www.patreon.com/join/RuneLitePlus')
      mat-icon.icon(aria-hidden='false', aria-label='Patreon', color='accent') favorite

  mat-toolbar-row.mobile-nav(fxHide.gt-xs="true", fxLayout="row wrap", fxLayoutAlign="flex-start")
    a(mat-button='' routerLink='/updates', fxFlex="0 1 50%") Updates
    a(mat-button='' routerLink='/features', fxFlex="0 1 50%") Features

router-outlet
